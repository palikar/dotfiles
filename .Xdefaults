!! Basic interface setup
URxvt.intensityStyles:	false
URxvt.background:       [90]#000000
URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	white
Rxvt.saveLines: 200000
URxvt.unicode3:		true

! URxvt.letterSpace: 0

!! Extensions
URxvt.perl-lib:							/home/arnaud/.config/urxvt-perls/
URxvt.perl-ext-common:			        default,matcher,resize-font,url-select,clipboard,font-sizee

! URxvt.font: 		xft:mono:size=12

! URxvt.font:								xft:Inconsolata:style=Bold:size=12
! URxvt.font:								xft:SauceCodePro Nerd Font Mono:style=Bold:size=12

URxvt.font:                                 xft:SauceCodePro Nerd Font Mono:style=Bold:size=12, xft:DejaVu Sans:pixelsize=14, xft:Inconsolata Nerd Font Mono:size=15, xft:Hack:pixelsize=18


URxvt.colorUL:							#4682B4	



URxvt.keysym.M-l:                       perl:url-select:select_next
URxvt.url-select.launcher:              sensible-browser
URxvt.url-select.underline:             true

!! keyboard-eslect:
URxvt.keysym.M-i:                       perl:keyboard-select:activate

URxvt.keyboard-select.clipboard:        true

!! resize-font
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1

!! Clipboard
URxvt.keysym.M-w:   perl:clipboard:copy
URxvt.keysym.C-y:   perl:clipboard:paste
URxvt.keysym.C-w:   perl:clipboard:paste_escaped

!! Good scrolling
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
urxvt*saveLines:            10000

