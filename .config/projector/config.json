
{
    "cmake-project":{
        "root_dir": "cmake_project",
        "properties": [
            {
                "name": "Name of the project",
                "token": "project_name"
            },
            {
                "name": "Installation prefix for 'make install' ",
                "token": "install_prefix",
                "default":"/usr/local/"
            },
            {
                "name": "Init git",
                "token" : "git",
                "y_n": true
            },
            {
                "name": "Licens",
                "token": "lic",
                "choice": ["GPL", "MIT"]
            }
        ]
    },
    
